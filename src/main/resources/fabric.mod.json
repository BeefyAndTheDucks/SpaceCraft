{
  "schemaVersion": 1,
  "id": "spacecraft",
  "version": "${version}",

  "name": "SpaceCraft",
  "description": "Mod for going to space",
  "authors": [
    "supertoad",
    "BeefyAndTheDucks"
  ],
  "contact": {
    "repo": "https://github.com/BeefyAndTheDucks/spacecraft"
  },
  "license": "MIT License",
  "icon": "assets/spacecraft/textures/icons/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.spacecraftteam.spacecraft.SpaceCraft"
    ],
    "client": [
      "com.spacecraftteam.spacecraft.client.SpaceCraftClient"
    ],
    "fabric-datagen": [
      "com.spacecraftteam.spacecraft.datagen.ModDataGeneration"
    ],
    "modmenu": [
      "com.spacecraftteam.spacecraft.impl.modmenu.SpaceCraftModMenu"
    ],
    "terrablender": [
      "com.spacecraftteam.spacecraft.impl.terrablender.SpaceCraftTerrablender"
    ]
  },
  "mixins": [
    "spacecraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=17",
    "terrablender": ">=2.3.0.22"
  },
  "suggests": {
    "modmenu": "*"
  }
}
